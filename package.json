{
  "name": "swimmingly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"server,scheduler\" -c \"blue,green\" \"npm run dev:server\" \"npm run dev:scheduler\"",
    "dev:server": "next dev",
    "dev:scheduler": "tsx scripts/static-update-scheduler.ts",
    "dev:no-updates": "ENABLE_STATIC_UPDATES=false npm run dev:server",
    "build": "next build",
    "build:static": "BUILD_MODE=static NEXT_PUBLIC_BUILD_MODE=static npm run generate-static-data && next build && bash scripts/export-static.sh",
    "generate-static-data": "tsx src/lib/static/generateStaticData.ts",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@deck.gl/core": "^9.1.1",
    "@deck.gl/layers": "^9.1.1",
    "@deck.gl/react": "^9.1.1",
    "@prisma/client": "^6.2.0",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "mapbox-gl": "^3.8.0",
    "next": "^15.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.0",
    "redis": "^4.7.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^3.4.0",
    "@types/node": "^22",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "^15.1.2",
    "node-cron": "^4.2.1",
    "postcss": "^8",
    "prisma": "^6.2.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
